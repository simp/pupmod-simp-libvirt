{
  "name": "simp-libvirt",
  "version": "5.3.0",
  "author": "SIMP Team",
  "summary": "manages libvirtd in a SIMP-compatible environment",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-libvirt",
